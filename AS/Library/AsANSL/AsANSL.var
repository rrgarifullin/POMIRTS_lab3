	asANSL_ERR_FILE_RESOURCE			: UINT		:= 31655;	(*system out of file resource*)
	
	asANSL_ERR_PARAM_INVALID			: UINT		:= 31870;	(*invalid parameter*)
	asANSL_ERR_XML_PARSER_INIT_ERROR	: UINT		:= 31871;	(*xml parser error*)
	asANSL_ERR_XML_INVALID				: UINT		:= 31872;	(*invalid xml configuration*)
