VAR CONSTANT
	asANSL_OPTION_FILTER_ALL 			: USINT		:= 0;		(*iterate over all connections*)
	asANSL_OPTION_FILTER_NOERROR 		: USINT		:= 1;		(*iterate over all connections without error*)
